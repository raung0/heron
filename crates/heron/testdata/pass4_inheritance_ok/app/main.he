package main

Base :: struct {
	pub pub_field: int
	pub priv_field: int

	pub pub_method :: fn (self: &Base) -> int do self.priv_field
	pub priv_method :: fn (self: &Base) -> int do self.priv_field
}

Child :: struct extends Base {
}

child_method := fn (self: Child) -> int {
	self.priv_field + self.priv_method()
}

main := fn {
	child := Child .{ }
	_ = child.pub_field
	_ = child_method(child)
}
