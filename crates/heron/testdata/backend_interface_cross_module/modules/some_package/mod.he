package some_package

pub Adder :: interface {
	pub add := fn (self: &Self; x: int) -> int ---
}

pub Counter :: struct {
	pub value: int

	pub add :: fn (self: &Self; x: int) -> int do self.value + x
}

pub run :: fn (seed: int) -> int {
	counter := Counter .{ .value = seed }
	iface := cast<Adder>counter
	iface.add(10)
}
